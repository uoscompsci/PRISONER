library(ggplot2)
library(scales)
box_cols = c("#ffffff","#999999","#555555")
font_size <- 16
# student box
dev.new(width=10,height=7)
stud_alt <- read.table("D:\\Downloads\\student_normalised_box_alt",header=T)
attach(stud_alt)
wall_factor <- factor(wall, levels=rev(levels(wall)))
p <- ggplot(stud_alt,aes(student,frequency), label)
p + geom_boxplot(aes(fill=wall_factor)) +
scale_y_continuous("Frequency",labels=percent_format()) +
scale_x_discrete("Student status") +
gttscale_fill_manual("",values=box_cols) +
theme_bw(base_size=font_size) +
opts(axis.text.x=theme_text(size=font_size),axis.text.y=theme_text(size=font_size),legend.key = theme_rect(col = 0),legend.text=theme_text(size=font_size),axis.title.x=theme_text(vjust=0,size=font_size,face="bold"),axis.title.y=theme_text(hjust=0.5,vjust=0.15,angle=90,size=font_size,face="bold"))
