<p:experimental-design
        xmlns:p="http://pvnets.cs.st-andrews.ac.uk/prisoner/experimental_design"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://pvnets.cs.st-andrews.ac.uk/prisoner/experimental_design
                experimental_design.xsd">

<experiment name="FacebookDummy">
<tables>
        <table name="participant" type="participant">
                <column name="name" type="string" />
                <column name="gender" type="string" />
                <column name="serviceGroup" type="string" />
        </table>

        <table name="response" type="response">
                <column name="answer" type="string" />
        </table>
</tables>
</experiment>
</p:experimental-design>

